[[ $TERM == "dumb" ]] && unsetopt zle && PS1='$ ' && return

export ZSH=$HOME/.zsh.d
export CASE_SENSITIVE="true"
export DISABLE_AUTO_TITLE="true"

plugins=(<%= @plugins.join(' ') %>)
completion=(<%= @completions.join(' ') %>)

pyenv_prompt=<%= @config['pyenv_prompt'] %>
rbenv_prompt=<%= @config['rbenv_prompt'] %>
ndenv_prompt=<%= @config['ndenv_prompt'] %>
shell_color1=<%= @config['color1'] %>
shell_color2=<%= @config['color2'] %>
theme=<%= @theme %>

source $ZSH/init.zsh

export LC_TYPE=en_US.UTF-8
export LC_ALL=en_US.UTF-8
